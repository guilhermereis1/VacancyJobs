<p id="notice"><%= notice %></p>

<h1>Application Vacancies</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Vacancy</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_vacancies.each do |application_vacancy| %>
      <tr>
        <td><%= application_vacancy.user_id %></td>
        <td><%= application_vacancy.vacancy_id %></td>
        <td><%= link_to 'Show', application_vacancy %></td>
        <td><%= link_to 'Edit', edit_application_vacancy_path(application_vacancy) %></td>
        <td><%= link_to 'Destroy', application_vacancy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Application Vacancy', new_application_vacancy_path %>
